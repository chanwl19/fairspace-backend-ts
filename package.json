  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/storage": "^7.11.3",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csurf": "^1.11.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-formidable": "^1.2.0",
    "express-validator": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.4",
    "multer": "^1.4.5-lts.1",
    "multer-cloud-storage": "^3.1.0",
    "multer-google-storage": "^1.3.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.7",
    "streamifier": "^0.1.1",
    "ts-node": "^10.9.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-formidable": "^1.2.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.12",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.4",
    "prettier": "3.3.2",
    "typescript": "^5.4.5"
  }
}
